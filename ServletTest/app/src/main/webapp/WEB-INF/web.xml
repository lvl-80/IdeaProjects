<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <error-page>
        <error-code>404</error-code>
        <location>/404err.html</location>
    </error-page>
    <context-param>
        <param-name>Haha</param-name>
        <param-value>Haha value</param-value>
    </context-param>
    
    <servlet>
        <servlet-name>MyTest1</servlet-name>
        <servlet-class>com.lvl80.servlet.MyTestServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>MyTest1</servlet-name>
        <url-pattern>/haha1</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>MyTest2</servlet-name>
        <servlet-class>com.lvl80.servlet.MyTestServlet2</servlet-class>
        <init-param>
            <param-name>Hello</param-name>
            <param-value>Hello World</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>MyTest2</servlet-name>
        <url-pattern>/haha2</url-pattern>
    </servlet-mapping>
</web-app>